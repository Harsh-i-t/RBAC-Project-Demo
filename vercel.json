{
	"version": 2,
	"name": "RBAC-Project-main",
	"builds": [
		{
			"src": "src/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/css/(.*)",
			"dest": "/public/css/$1"
		},
		{
			"src": "/(.*)",
			"dest": "src/index.js"
		},
		    {
		      "src": "/js/(.*)",
		      "dest": "/public/js/$1"
		    }
	]
}
